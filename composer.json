{
  "name": "np21/form",
  "description": "PHPなフォームメールを作りやすいようにするライブラリ",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "takuma kanayama",
      "email": "takuma_kanayama@netpark21.co.jp"
    }
  ],
  "require": {
    "phpmailer/phpmailer": "^6.0",
    "ext-mbstring": "*",
    "php": "^5.6.37 || ^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0 || ^7.0 || ^8.0"
  },
  "autoload": {
    "psr-4": {
      "NP21\\Form\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "NP21\\Test\\": "Test"
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
